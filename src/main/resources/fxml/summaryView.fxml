<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" minHeight="391.0" minWidth="850.0" styleClass="white-background" stylesheets="@../css/Stylesheet.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="TOP_RIGHT">
         <children>
            <Button mnemonicParsing="false" onAction="#switchToInputView" text="Till Detalj Vy" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets right="20.0" top="20.0" />
               </VBox.margin></Button>
         </children>
      </VBox>
      <Label fx:id="titleLabel" text="Projektnamn + Sammanfattning">
         <VBox.margin>
            <Insets bottom="20.0" top="20.0" />
         </VBox.margin>
      </Label>
      <HBox alignment="TOP_CENTER" VBox.vgrow="ALWAYS">
         <children>
            <VBox alignment="CENTER" minWidth="300.0" HBox.hgrow="ALWAYS">
               <children>
                  <Label text="Lägenhetsdata">
                     <VBox.margin>
                        <Insets bottom="20.0" top="20.0" />
                     </VBox.margin>
                  </Label>
                  <Button minWidth="200.0" mnemonicParsing="false" onAction="#addNewApartmentItem" text="Lägg till lägenhetstyp">
                     <VBox.margin>
                        <Insets left="150.0" right="150.0" />
                     </VBox.margin>
                  </Button>
                  <ScrollPane fitToHeight="true" fitToWidth="true" hbarPolicy="NEVER" VBox.vgrow="ALWAYS">
                     <content>
                        <VBox fx:id="lagenhetsTypVbox" alignment="TOP_CENTER" spacing="5.0" />
                     </content>
                  </ScrollPane>
               </children>
            </VBox>
            <VBox alignment="TOP_CENTER" minWidth="550.0" HBox.hgrow="ALWAYS">
               <children>
                  <Label text="Nyckeltal">
                     <VBox.margin>
                        <Insets bottom="20.0" top="20.0" />
                     </VBox.margin>
                  </Label>
                  <VBox alignment="CENTER" maxHeight="500.0" spacing="20.0" VBox.vgrow="ALWAYS">
                     <children>
                        <HBox alignment="CENTER" VBox.vgrow="ALWAYS">
                           <children>
                              <Label>
                                 <HBox.margin>
                                    <Insets right="100.0" />
                                 </HBox.margin>
                              </Label>
                              <Label text="Med stöd" HBox.hgrow="ALWAYS">
                                 <HBox.margin>
                                    <Insets left="90.0" right="60.0" />
                                 </HBox.margin>
                              </Label>
                              <Label text="Utan stöd" HBox.hgrow="ALWAYS">
                                 <HBox.margin>
                                    <Insets left="50.0" />
                                 </HBox.margin>
                              </Label>
                           </children>
                        </HBox>
                        <HBox alignment="CENTER" spacing="10.0" VBox.vgrow="ALWAYS">
                           <children>
                              <Label alignment="CENTER_RIGHT" prefWidth="200.0" text="Projektkostnad:">
                                 <font>
                                    <Font size="24.0" />
                                 </font>
                                 <HBox.margin>
                                    <Insets />
                                 </HBox.margin>
                              </Label>
                              <TextField fx:id="projektkostnadMedStod" alignment="CENTER" editable="false" maxWidth="150.0" minWidth="150.0" style="-fx-background-color: lightgrey;">
                                 <cursor>
                                    <Cursor fx:constant="DEFAULT" />
                                 </cursor>
                                 <font>
                                    <Font size="24.0" />
                                 </font>
                                 <HBox.margin>
                                    <Insets />
                                 </HBox.margin>
                              </TextField>
                              <TextField fx:id="projektkostnadUtanStod" alignment="CENTER" editable="false" maxWidth="150.0" minWidth="150.0" style="-fx-background-color: lightgrey;">
                                 <cursor>
                                    <Cursor fx:constant="DEFAULT" />
                                 </cursor>
                                 <font>
                                    <Font size="24.0" />
                                 </font>
                              </TextField>
                           </children>
                           <padding>
                              <Insets right="20.0" />
                           </padding>
                        </HBox>
                        <HBox alignment="CENTER" spacing="10.0" VBox.vgrow="ALWAYS">
                           <children>
                              <Label alignment="CENTER_RIGHT" prefWidth="200.0" text="Marknadsvärde:">
                                 <font>
                                    <Font size="24.0" />
                                 </font>
                                 <HBox.margin>
                                    <Insets />
                                 </HBox.margin>
                              </Label>
                              <TextField fx:id="marknadsvardeMedStod" alignment="CENTER" editable="false" maxWidth="150.0" minWidth="150.0" style="-fx-background-color: lightgrey;">
                                 <cursor>
                                    <Cursor fx:constant="DEFAULT" />
                                 </cursor>
                                 <font>
                                    <Font size="24.0" />
                                 </font>
                                 <HBox.margin>
                                    <Insets />
                                 </HBox.margin>
                              </TextField>
                              <TextField fx:id="marknadsvardeUtanStod" alignment="CENTER" editable="false" maxWidth="150.0" minWidth="150.0" style="-fx-background-color: lightgrey;">
                                 <cursor>
                                    <Cursor fx:constant="DEFAULT" />
                                 </cursor>
                                 <font>
                                    <Font size="24.0" />
                                 </font>
                              </TextField>
                           </children>
                           <padding>
                              <Insets right="20.0" />
                           </padding>
                        </HBox>
                        <HBox alignment="CENTER" spacing="10.0" VBox.vgrow="ALWAYS">
                           <children>
                              <Label alignment="CENTER_RIGHT" prefWidth="200.0" text="Projektvinst:">
                                 <font>
                                    <Font size="24.0" />
                                 </font>
                                 <HBox.margin>
                                    <Insets />
                                 </HBox.margin>
                              </Label>
                              <TextField fx:id="projektvinstMedStod" alignment="CENTER" editable="false" maxWidth="150.0" minWidth="150.0" style="-fx-background-color: lightgrey;">
                                 <cursor>
                                    <Cursor fx:constant="DEFAULT" />
                                 </cursor>
                                 <font>
                                    <Font size="24.0" />
                                 </font>
                                 <HBox.margin>
                                    <Insets />
                                 </HBox.margin>
                              </TextField>
                              <TextField fx:id="projektvinstUtanStod" alignment="CENTER" editable="false" maxWidth="150.0" minWidth="150.0" style="-fx-background-color: lightgrey;">
                                 <cursor>
                                    <Cursor fx:constant="DEFAULT" />
                                 </cursor>
                                 <font>
                                    <Font size="24.0" />
                                 </font>
                              </TextField>
                           </children>
                           <padding>
                              <Insets right="20.0" />
                           </padding>
                        </HBox>
                        <HBox alignment="CENTER" spacing="10.0" VBox.vgrow="ALWAYS">
                           <children>
                              <Label alignment="CENTER_RIGHT" prefWidth="200.0" text="Projektvinst i %:">
                                 <font>
                                    <Font size="24.0" />
                                 </font>
                                 <HBox.margin>
                                    <Insets />
                                 </HBox.margin>
                              </Label>
                              <TextField fx:id="projektvinstProcentMedStod" alignment="CENTER" editable="false" maxWidth="150.0" minWidth="150.0" style="-fx-background-color: lightgrey;">
                                 <cursor>
                                    <Cursor fx:constant="DEFAULT" />
                                 </cursor>
                                 <font>
                                    <Font size="24.0" />
                                 </font>
                                 <HBox.margin>
                                    <Insets />
                                 </HBox.margin>
                              </TextField>
                              <TextField fx:id="projektvinstProcentUtanStod" alignment="CENTER" editable="false" maxWidth="150.0" minWidth="150.0" style="-fx-background-color: lightgrey;">
                                 <cursor>
                                    <Cursor fx:constant="DEFAULT" />
                                 </cursor>
                                 <font>
                                    <Font size="24.0" />
                                 </font>
                              </TextField>
                           </children>
                           <padding>
                              <Insets right="20.0" />
                           </padding>
                        </HBox>
                     </children>
                     <padding>
                        <Insets bottom="20.0" top="20.0" />
                     </padding>
                  </VBox>
               </children>
            </VBox>
         </children>
      </HBox>
